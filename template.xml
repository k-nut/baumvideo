<Map srs="+proj=utm +zone=33 +ellps=GRS80 +units=m +no_defs"
     background-color="#00000000">
  <Style name="trees" filter-mode="first">
    <Rule>
      <MarkersSymbolizer width="5" fill="#009900" stroke="#881133" allow-overlap="true" ignore-placement="true" />
    </Rule>
  </Style>
  <Layer name="trees" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
      <StyleName>trees</StyleName>
      <Datasource>
        <Parameter name="file">/Users/knut/projects/baumvideo/data/YEAR.json</Parameter>
         <Parameter name="type">geojson</Parameter>
      </Datasource>
  </Layer>
</Map>
